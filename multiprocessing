from multiprocessing import Process
import time
import numpy as np


epoch_time = int(time.time())

print(epoch_time)

def run_it(phi):
    print(phi)
    time.sleep(5)
    
    
if __name__ == '__main__':
    
    jobs=[]
    
    for phi in np.arange(0.1, 1, 0.1):
        print(phi)
        for _ in range(2):
            print(_, phi)
            jobs.append(Process(target=run_it,args=(phi,)))


    for job in jobs:
        job.start()

    for job in jobs:
        job.join()
