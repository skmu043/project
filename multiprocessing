from multiprocessing import Process
import time
import numpy as np


epoch_time = int(time.time())

print(epoch_time)

def run_it(phi):
    print(phi)
    time.sleep(5)
    
    
if __name__ == '__main__':
    
    for phi in np.arange(0.1, 1, 0.1):
        print(phi)
        for _ in range(100):
            print(_, phi)
            p = Process(target=run_it)
            p.start()
            p.join()
